import java.util.Scanner;

public class month {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		outerloop: while (true) {
			System.out
					.println("Ange nummret på den månad du vill visa dagar och namn för: ");
			int month = Integer.parseInt(scan.nextLine());
			String monthString, days;
			if (month == 0) {
				break outerloop;
			} else {
				switch (month) {
				case 1:
					monthString = "Januari";
					days = "31";
					break;
				case 2:
					monthString = "Februari";
					days = "29";
					break;
				case 3:
					monthString = "Mars";
					days = "31";
					break;
				case 4:
					monthString = "April";
					days = "30";
					break;
				case 5:
					monthString = "Maj";
					days = "31";
					break;
				case 6:
					monthString = "Juni";
					days = "30";
					break;
				case 7:
					monthString = "Juli";
					days = "31";
					break;
				case 8:
					monthString = "Augusti";
					days = "31";
					break;
				case 9:
					monthString = "September";
					days = "30";
					break;
				case 10:
					monthString = "Oktober";
					days = "31";
					break;
				case 11:
					monthString = "November";
					days = "30";
					break;
				case 12:
					monthString = "December";
					days = "31";
					break;
				default:
					monthString = "Inte ett accepterat nummer, 0 < nummer > 12";
					days = "";
					break;

				}
			}
			System.out.println("månad: " + monthString + "\ndagar: " + days+"\n");
		}

	}
}
